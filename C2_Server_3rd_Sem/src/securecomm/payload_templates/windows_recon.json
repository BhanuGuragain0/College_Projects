    {
      "type": "exec",
      "payload": "net user",
      "description": "List local user accounts",
      "timeout": 30
    },
    {
      "type": "exec",
      "payload": "net localgroup administrators",
      "description": "List admin group members",
      "timeout": 30
    },
    {
      "type": "exec",
      "payload": "tasklist /v",
      "description": "List running processes with details",
      "timeout": 60
    },
    {
      "type": "exec",
      "payload": "netstat -ano",
      "description": "Show network connections",
      "timeout": 60
    },
    {
      "type": "exec",
      "payload": "schtasks /query /fo LIST /v",
      "description": "List scheduled tasks",
      "timeout": 60
    },
    {
      "type": "exec",
      "payload": "sc query state= all",
      "description": "List all services",
      "timeout": 60
    },
    {
      "type": "exec",
      "payload": "netsh advfirewall show allprofiles",
      "description": "Get firewall configuration",
      "timeout": 60
    },
    {
      "type": "exec",
      "payload": "wmic product get name,version",
      "description": "List installed software",
      "timeout": 120
    },
    {
      "type": "exec",
      "payload": "reg query HKLM\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Run",
      "description": "Check startup registry keys",
      "timeout": 30
    }
